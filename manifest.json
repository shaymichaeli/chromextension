{
  "manifest_version": 2,

  "name": "lookUp",
  "description": "select what ever you want and search it",
  "version": "1.0",
  //"background": {
  //  "scripts" : ["background.js"]
  //},

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus", "tabs", "activeTab", "history"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["get_information.js"]
    }
  ]
}

